#include <iostream>
#include <bits/stdc++.h>
using namespace std;
 
int main()
{
   string s,t;
   int len=0,testCases;
   cin>>testCases;
   while(testCases--)
   {
   	t="";
   cin>>s;
   len=s.length();
   t+=s[0];
 
   for(int i=1;i<len ;i+=2)
   {
     t+=s[i];
   }
   cout<<t<<endl;
   }
    return 0;
}
